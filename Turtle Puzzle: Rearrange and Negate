#include<bits/stdc++.h>

#define all(x) x.begin(), x.end()
#define all_g(x) x.rbegin(), x.rend()
#define Faster ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);

using namespace std;

using ll = long long;
const int N = 1e5 + 10;

void solve()
{
    int n;
    cin>>n;

    int sum = 0;
    vector<ll>v(n);
    for(int i=0; i<n; i++)
    {
        cin>>v[i];
        sum+= (abs(v[i]));
    }
    cout<<sum<<"\n";

}


int main()
{
    Faster
    int t;
    cin>>t;
    while(t--)
    {
        solve();
    }
    return 0;
}


