void solve()
{
    int n,t;
    cin>>n;

    set<ll>x;
    for(int i=0; i<n; i++)
    {
        cin>>t;
        x.insert(t);
    }
    cout<<x.size() <<"\n";
}


int main()
{
    Faster

    solve();
    return 0;
