#include<bits/stdc++.h>

#define all(x) x.begin(), x.end()
#define all_g(x) x.rbegin(), x.rend()
#define Faster ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);

using namespace std;

using ll = long long;
const int N = 1e5 + 10;

void solve()
{
    int n,one=0,zero=0;
    string a;
    cin>>n>>a;
    for(int i=0; i<n; i++)
    {
        if (a[i]=='n') one++;
        if (a[i]=='z') zero++;
    }
    for(int i=0; i<one; i++)
    {
        cout <<"1 ";
    }
    for(int i=0; i<zero; i++)
    {
        cout <<"0 ";
    }
}


int main()
{
    Faster

    solve();
    return 0;
}

