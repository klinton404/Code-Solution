#include<bits/stdc++.h>

#define all(x) x.begin(), x.end()
#define all_g(x) x.rbegin(), x.rend()
#define Faster ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);

using namespace std;

using ll = long long;
const int N = 1e5 + 10;

void solve()
{
    int n;
    cin>>n;
    vector<int>a(n);
    for(int i=0; i<n; i++)
    {
        cin>>a[i];
    }
    cout<<a[0]<<"\n";
}


int main()
{
    Faster;
    int t;
    cin>>t;
    while(t--)
    {
        solve();
    }
    return 0;
}
