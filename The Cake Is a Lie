#include<bits/stdc++.h>

#define pb push_back
#define all(x) x.begin(), x.end()
#define Faster ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);
#define yes cout<<"YES"<<"\n"
#define no cout<<"NO"<<"\n"
using namespace std;



using ll = long long;
const int N = 1e5 + 10;

void solve()
{
    ll n, m, k;
    cin>>n>>m>>k;
    if(n*m-1==k)
    {
        yes;
    }
    else
        no;
}


int main()
{
    Faster;
    int t;
    cin>>t;
    while(t--)
    {
        solve();
    }
    return 0;
}

