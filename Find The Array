#include<bits/stdc++.h>

#define all(x) x.begin(), x.end()
#define all_g(x) x.rbegin(), x.rend()
#define Faster ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);

using namespace std;

using ll = long long;
const int N = 1e5 + 10;

void solve()
{
    int n;
    cin>>n;
    int sum=0;
    int cnt=0;
    int i=1;
    while(sum<n)
    {
        cnt++;
        sum+=i;
        i+=2;
    }
    cout<<cnt<<endl;
}


int main()
{
    Faster;
    int t;
    cin>>t;
    while(t--)
    {
        solve();
    }
    return 0;
}

